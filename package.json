{
  "name": "Student_Details_Joule",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/http-client": "^4.1.2",
    "@sap-cloud-sdk/util": "^4.1.2",
    "@sap/cds": "^9",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.10.0",
    "express": "^4",
    "hdb": "^2.26.1",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/dev-cap-tools": "^1.31.0",
    "cds-plugin-ui5": "^0.13.0",
    "@sap/cds-dk": "^9",
    "rimraf": "^5.0.5",
    "mbt": "^1.2.29"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-school-management": "cds watch --open schoolmanagement/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-school": "cds watch --open managestudent.school/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy Student_Details_Joule --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "lcap": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/school-management",
    "app/school"
  ]
}
